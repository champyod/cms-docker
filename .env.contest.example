# =============================================================================
# CMS Contest Web Server Environment Configuration
# Author: CCYod | Repository: https://github.com/champyod/cms-docker
# =============================================================================
# ALL CONTEST SETTINGS ARE MANAGED VIA THE ADMIN PANEL UI
# These variables are automatically updated by the Admin Panel.

# JSON configuration for deployed contest instances
CONTESTS_DEPLOY_CONFIG=[]

# Global security key for session management
SECRET_KEY=

# Default settings (inherited by new contests)
COOKIE_DURATION=10800
ACCESS_METHOD=public_port
NUM_PROXIES_USED=1
MAX_SUBMISSION_LENGTH=100000
MAX_INPUT_LENGTH=5000000
SUBMIT_LOCAL_COPY=true

# Infrastructure defaults
CONTEST_WEB_CPU_LIMIT=2
CONTEST_WEB_MEMORY_LIMIT=2G
ENABLE_TLS=false